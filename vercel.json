{
  "builds": [
    { "src": "typehead-backend/server.js", "use": "@now/node" },
    { "src": "typehead-frontend/package.json", "use": "@now/static-build" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "typehead-backend/server.js" },
    { "src": "/(.*)", "dest": "typehead-frontend/public/$1" }
  ]
}
